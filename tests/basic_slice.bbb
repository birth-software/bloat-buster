require = fn (ok: u1) void
{
    if (!ok)
    {
        #trap();
    }
}

slice_receiver = fn (slice: []u8) void
{
    require(slice.length == 3);
    require(slice[0] == 0);
    require(slice[1] == 1);
    require(slice[2] == 2);
}

[export] main = fn [cc(c)] () s32
{
    >a: [_]u8 = [0, 1, 2];
    slice_receiver(&a);
    return 0;
}
