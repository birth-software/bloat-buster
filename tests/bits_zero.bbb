require = fn (ok: u1) void
{
    if (!ok)
    {
        #trap();
    }
}

S = bits
{
    a: u1,
    b: u1,
    c: u1,
}

[export] main = fn () s32
{
    >a: S = zero;

    require(a.a == 0);
    require(a.b == 0);
    require(a.c == 0);

    >b: S = {
        .a = 1,
        .b = 1,
        zero,
    };

    require(b.a == 1);
    require(b.b == 1);
    require(b.c == 0);

    return 0;
}
