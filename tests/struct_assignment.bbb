S1 = struct
{
    a: u8,
    b: u8,
    c: u8,
}

S2 = struct
{
    a: u8,
    b: u8,
    c: u8,
}

require = fn (ok: u1) void
{
    if (!ok)
    {
        #trap();
    }
}

[export] main = fn [cc(c)] () s32
{
    >s1: S1 = {
        .a = 255,
        .b = 254,
        .c = 253,
    };

    >s2 :S2 = {
        .a = s1.a,
        .b = s1.b,
        .c = s1.c,
    };

    require(s1.a == 255);
    require(s1.b == 254);
    require(s1.c == 253);

    require(s2.a == 255);
    require(s2.b == 254);
    require(s2.c == 253);

    return 0;
}
